//
// Build the setup compat library.
//

android_library {
    name: "setup-compat",
    manifest: "AndroidManifest.xml",
    resource_dirs: [
        "main/res",
    ],
    sdk_version: "current",
    srcs: [
        "main/java/**/*.java",
    ],
    static_libs: [
        "androidx.annotation_annotation",
        "guava",
    ],
    min_sdk_version: "14",
}
